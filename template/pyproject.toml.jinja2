[project]
name = "airflow-provider-{{ name }}"
authors = [

]
description = "A Apache Airflow provider package"
classifiers = [
    "Framework :: Apache Airflow",
    "Framework :: Apache Airflow :: Provider",
]
dynamic = [
    "version"
]
requires-python = "~=3.8"
dependencies = [
    "apache-airflow>=2.9"
]

[project.readme]
file = "README.md"
content-type = "text/markdown"

[project.entry-points.apache_airflow_provider]
provider_info = "airflow_provider_{{ name }}.__init__:get_provider_info"


[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"